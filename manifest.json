{
    "description": "Adds browser action icon to toolbar to open Tidal web page.",
    "manifest_version": 2,
    "name": "Open Tidal web page",
    "version": "1.0",
    "homepage_url": "https://listen.tidal.com",
    "icons": {
      "48": "icons/tidal.png"
    },
  
    "background": {
      "scripts": ["background.js"]
    },
  
    "browser_action": {
      "default_icon": "icons/tidal.png"
    }
}